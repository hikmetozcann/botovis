{
    "name": "botovis/botovis",
    "description": "Botovis SDK for Laravel",
    "keywords": ["botovis", "laravel", "sdk"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Botovis\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Botovis\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Botovis\\BotovisServiceProvider"
            ],
            "aliases": {
                "Botovis": "Botovis\\Facades\\Botovis"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit",
        "analyse": "phpstan analyse src --level=8"
    }
}
